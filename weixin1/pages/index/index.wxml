<view class="container">

  
 <view class="page-section">
        <view class="page-section-title">姓名</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd" >
             <view class='border'><input class="weui-input" name="name" placeholder="" maxlength='4' /></view>
            </view>
          </view>
        </view>
      </view>

      <view class="page-section page-section-gap">
        <view class="page-section-title">性别</view>
     <view class="border">
        <radio-group name="radio" >
          <label><radio value="radio1"  color="#abcdef" />男</label>
          <label><radio value="radio2"  color="#abcdef"/>女</label>
        </radio-group>
      </view>
      </view>
      
     
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="page-section-title">出生日期</view>
          </view>
            <view class="weui-cell__bd">
            <picker mode="date" value="{{date}}" start="1998-01-01" end="2003-12-12" bindchange="bindDateChange">
            <view class="border"><view class="weui-input">当前选择：{{date}}</view></view>
            </picker>
          </view>
        </view>
        
        <view class="page-section">
        <view class="page-section-title">学号</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd" >
             <view class='border' ><input class="weui-input" name="input" maxlength='10' /></view>
            </view>
          </view>
        </view>
         </view>
        </view>

        <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__hd">
            <view class="page-section-title">籍贯</view>
          </view>
          <view class="weui-cell__bd">
            <picker mode="region" value="{{region}}"  bindchange="bindRegionChange" custom-item="{{customItem}}">
             <view class='border'><view class="weui-input">当前选择：</view></view> 
            </picker>
          </view>
        </view>
        </view>

        <view class="page-section">
        <view class="page-section-title">宿舍地址</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd" >
             <view class='border'><input class="weui-input" name="input"/>  </view>
            </view>
          </view>
        </view>
      </view> 

       <view class="page-section">
        <view class="page-section-title">专业班级</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd" >
             <view class='border'><input class="weui-input" name="input"  /></view>
            </view>
          </view>
        </view>
      </view>

       <view class="page-section">
        <view class="page-section-title">电子邮箱</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd" >
             <view class='border'><input class="weui-input" name="email" placeholder="" maxlength='12'style="height: 2em" /></view>
            </view>
          </view>
        </view>
      </view>

       <view class="page-section">
        <view class="page-section-title">联系电话</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd" >
             <view class='border'><input class="weui-input" name="mobile" placeholder="" maxlength='11'style="height: 2em" /></view>
            </view>
          </view>
        </view>
      </view>

      <view class="page-section-title">志愿部门</view>
      <view class="weui-cells weui-cells_after-title">
        <view class="weui-cell weui-cell_input">
          <view class="weui-cell__bd">
            <picker bindchange="bindPickerChange" value="{{index}}" range="{{department}}">
             <view class='border'><view class="weui-input">当前选择： </view></view> 
            </picker>
          </view>
        </view>
      </view>
      

       <view class="page-section">
        <view class="page-section-title">爱好特长</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd" >
             <view class='title' ><input class="weui-input" name="input" maxlength='30' style="height: 2em" /></view>
             </view>
            </view>
          </view>
         </view>

         <view class="page-section">
        <view class="page-section-title" >自我评价</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd" >
             <view class='title' style="height: 10em"><input class="weui-input" name="input" maxlength='100' /></view>
             </view>
            </view>
          </view>
         </view>

         <view class="page-section">
        <view class="page-section-title">实践经历</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd" >
             <view class='title' style="height: 10em"><input class="weui-input" name="input" maxlength='100' /></view>
             </view>
            </view>
          </view>
         </view>

      
  
       <view class="page-section">
      <view class="page-section-title">你对社联的看法</view>
        <view class="weui-cells weui-cells_after-title">
          <view class="weui-cell weui-cell_input">
            <view class="weui-cell__bd" >
             <view class='title'style="height: 10em" ><input class="weui-input" name="input" maxlength='100' /></view>
             </view>
            </view>
          </view>
         </view>

      <view class="btn-area">
      <button type="primary" formType="submit">提交报名表</button>
    </view>

</view>
